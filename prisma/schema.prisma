datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URI") 
}

generator client {
    provider = "prisma-client-js"
}

enum Status {
  pending
  succeeded
  failed
  canceled
}

model Booking {
  id Int @id @default(autoincrement())
  ride_id Int
  user_id Int
  price Int
  successful Boolean @default(false)
  status Status @default(pending)
}